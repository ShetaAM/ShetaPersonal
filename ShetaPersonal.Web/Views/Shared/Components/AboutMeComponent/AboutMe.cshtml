@model ShetaPersonal.Core.DTO.AboutViewModel;


@if (ViewBag.success!=null)
{
    <div class="alert alert-success">
        <p>پیام شما با وفقیت ارسال گردید.</p>
    </div>
}
<div class="col-md-12 col-sm-6">
        <div>
            <div class="card">
                <div class="front">
                    <div class="cover">
                        <img src="/Site/img/backabout.jpg">
                    </div>
                    <div class="user">
                        <img class="img-circle" src="/Site/img/@Model.Profile">
                    </div>
                    <div class="content">
                        <div class="main">
                            <h5 class="name">@Model.FName@Model.LName</h5>
                            <h5 class="name">@Model.FNameEN @Model.LNameEN</h5>
                            <p class="profession">Full Stack Developer</p>
                            <p class="profession">تاریخ تولد:@Model.birthday</p>
                            <p class="text-center">@Model.AboutMe</p>
                        </div>
                    </div>
                </div> <!-- end front panel -->
            </div> <!-- end card -->
        </div> <!-- end card-container -->
    </div>

